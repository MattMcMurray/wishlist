<div class="w-full">
  <% content_for :title, "Lists" %>
  <h1 class="font-bold text-4xl mb-8">Lists</h1>

  <div id="lists" class="min-w-full flex flex-col gap-4">
    <% @lists.each do |list| %>
      <%= render list %>
    <% end %>
  </div>

  <%= turbo_frame_tag id="new_list", src:new_list_path %>
</div>
